<!DOCTYPE html>
<html>
<head>
  <title>Lista de Usuários</title>
</head>
<body>
  <h1>Lista de Usuários</h1>
  <ul>
    <% usuarios.forEach(function(usuario) { %>
      <li><%= usuario.nome %> - <%= usuario.email %> [<a href="/edit/<%= usuario.id %>">Editar</a>] [<a href="/delete/<%= usuario.id %>">Excluir</a>]</li>
    <% }); %>
  </ul>
  <a href="/create">Adicionar Novo Usuário</a>
</body>
</html>
